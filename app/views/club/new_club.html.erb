<form action="/club/create_club/" method="post" enctype="multipart/form-data">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    이미지 : <input type="file" name="img" accept="image/*"><br>
    단체 이름 : <input type="text" name="club_name"><br>
    소개 : <input type="text" name="introduce"><br>
    <input type="submit" value="제출">
</form>